#process.triggerstudies = cms.EDAnalyzer('TriggerStudies'
#)


process.PFHT900 = cms.EDAnalyzer("TriggerStudies",
triggerPath = cms.string( "HLT_PFHT900_v1" ),
patJets = cms.InputTag( "patJetsAK4CHS" ),
minHT = cms.double("950"),
minMass = cms.double("100")
)


process.p = cms.Path(process.PFHT900)
